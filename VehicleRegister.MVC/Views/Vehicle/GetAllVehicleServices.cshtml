@model IEnumerable<VehicleRegister.MVC.Models.GetVehicleServicesModel>

<p>
    @Html.ActionLink("View servicehistory", "GetAllServiceHistories")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.VehicleServiceId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VehicleId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServiceType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleServiceId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VehicleId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ServiceType)
        </td>
        <td>
            @Html.ActionLink("Details", "GetVehicleServiceById", new { item.VehicleServiceId/* id=item.PrimaryKey */ }) |
            @Html.ActionLink("Delete", "DeleteVehicleService", new { item.VehicleId, item.VehicleServiceId, item.ServiceDate, item.ServiceType })
        </td>
    </tr>
}

</table>
